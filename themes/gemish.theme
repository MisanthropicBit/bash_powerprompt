__bpp_set_theme() {
    BASH_POWERPROMPT_THEME='gemish'
    BASH_POWERPROMPT_FG_COLORS=(218 218 218 218 218)
    BASH_POWERPROMPT_BG_COLORS=(160 160 160 160 160)
    BASH_POWERPROMPT_SEPARATOR_FG_COLORS=(81 81 81 81 81)
    BASH_POWERPROMPT_SEPARATORS=(
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_SOLID_ARROW_SYMBOL
    )
    BASH_POWERPROMPT_COMMAND_SYMBOL=$(printf "\xf0\x9f\x92\x8e") # Diamond (U+1f48e)
    BASH_POWERPROMPT_GIT_FG_CLEAN_COLOR=81
    BASH_POWERPROMPT_GIT_FG_DIRTY_COLOR=81
    BASH_POWERPROMPT_RIGHT_PADDING=(' ' ' ' ' ' ' ' '  ')
}
