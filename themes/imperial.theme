#!/usr/bin/env bash

__bpp_set_theme() {
    BASH_POWERPROMPT_THEME='imperial'

    IGNORE_EMPTY_SECTIONS=0

    BASH_POWERPROMPT_FG_COLORS=(37 33 136 166 64)
    BASH_POWERPROMPT_BG_COLORS=(230 230 230 230 230)
    BASH_POWERPROMPT_SEPARATORS=(
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_THIN_ARROW_SYMBOL
        $BASH_POWERPROMPT_SOLID_ARROW_SYMBOL
    )
    BASH_POWERPROMPT_SEPARATOR_FG_COLOR=246
}
